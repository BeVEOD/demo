<tr><th id="uncatched_exceptions_exception_class_th"><%= _(%q{Exception Class}) %></th><th id="uncatched_exceptions_controller_name_th"><%= _(%q{Controller Name}) %></th><th id="uncatched_exceptions_action_name_th"><%= _(%q{Action Name}) %></th><th id="uncatched_exceptions_message_th"><%= _(%q{Message}) %></th><th id="uncatched_exceptions_backtrace_th"><%= _(%q{Backtrace}) %></th><th id="uncatched_exceptions_environment_th"><%= _(%q{Environment}) %></th><th id="uncatched_exceptions_request_th"><%= _(%q{Request}) %></th><th id="uncatched_exceptions_created_at_th"><%= _(%q{Created At}) %></th><th class="no-sort" rowspan="1"></th></tr>
</thead>

<% for key in @uncatched_exceptions_groups.keys %>

	<tr><th colspan="10" align="center" class="group"><%= key.nil? ? _('not set') : (key.respond_to?(:disp_name) ? key.disp_name : key.to_s.humanize ) %></th></tr>

<% @uncatched_exceptions_groups[key].each_with_index do |row,idx|
@uncatched_exception = row %>

		<tr class="<%= (idx % 2 == 1) ? 'odd' : 'even' %>" id=uncatched_exceptions_<%= @uncatched_exception.try(:id) %> >
<td><span class="aligned"><%= h @uncatched_exception.exception_class %></span></td>
<td><span class="aligned"><%= h @uncatched_exception.controller_name %></span></td>
<td><span class="aligned"><%= h @uncatched_exception.action_name %></span></td>
<td><span class="aligned"><%=  txt= @uncatched_exception.message; h(txt.to(100)) + (txt.length > 100 ? '...' : '') unless txt.blank? %></span></td>
<td><span class="aligned"><%=  txt= @uncatched_exception.backtrace; h(txt.to(100)) + (txt.length > 100 ? '...' : '') unless txt.blank? %></span></td>
<td><span class="aligned"><%=  txt= @uncatched_exception.environment; h(txt.to(100)) + (txt.length > 100 ? '...' : '') unless txt.blank? %></span></td>
<td><span class="aligned"><%=  txt= @uncatched_exception.request; h(txt.to(100)) + (txt.length > 100 ? '...' : '') unless txt.blank? %></span></td>
<td><span class="aligned"><%= h @uncatched_exception.created_at %></span></td>
<td class="std_links"><%= links_for_list_element(@uncatched_exception) %></td>
</tr>
<% end %><% end %></table>
<% end %>
<% unless(@uncatched_exceptions_groups) %>
<table class="sortable" id="uncatched_exceptions_table" summary="uncatched_exceptions list"><thead>
<tr><th id="uncatched_exceptions_exception_class_th"><%= _(%q{Exception Class}) %></th><th id="uncatched_exceptions_controller_name_th"><%= _(%q{Controller Name}) %></th><th id="uncatched_exceptions_action_name_th"><%= _(%q{Action Name}) %></th><th id="uncatched_exceptions_message_th"><%= _(%q{Message}) %></th><th id="uncatched_exceptions_backtrace_th"><%= _(%q{Backtrace}) %></th><th id="uncatched_exceptions_environment_th"><%= _(%q{Environment}) %></th><th id="uncatched_exceptions_request_th"><%= _(%q{Request}) %></th><th id="uncatched_exceptions_created_at_th"><%= _(%q{Created At}) %></th><th class="no-sort" rowspan="1"></th></tr>
</thead>

<tbody><% @uncatched_exceptions.each_with_index do |row,idx|
@uncatched_exception = row %>

		<tr class="<%= (idx % 2 == 1) ? 'odd' : 'even' %>" id=uncatched_exceptions_<%= @uncatched_exception.try(:id) %> >
<td><span class="aligned"><%= h @uncatched_exception.exception_class %></span></td>
<td><span class="aligned"><%= h @uncatched_exception.controller_name %></span></td>
<td><span class="aligned"><%= h @uncatched_exception.action_name %></span></td>
<td><span class="aligned"><%=  txt= @uncatched_exception.message; h(txt.to(100)) + (txt.length > 100 ? '...' : '') unless txt.blank? %></span></td>
<td><span class="aligned"><%=  txt= @uncatched_exception.backtrace; h(txt.to(100)) + (txt.length > 100 ? '...' : '') unless txt.blank? %></span></td>
<td><span class="aligned"><%=  txt= @uncatched_exception.environment; h(txt.to(100)) + (txt.length > 100 ? '...' : '') unless txt.blank? %></span></td>
<td><span class="aligned"><%=  txt= @uncatched_exception.request; h(txt.to(100)) + (txt.length > 100 ? '...' : '') unless txt.blank? %></span></td>
<td><span class="aligned"><%= h @uncatched_exception.created_at %></span></td>
<td class="std_links"><%= links_for_list_element(@uncatched_exception) %></td>
</tr>
<% end %></tbody></table>
<% end %>
 </div>
</div>
<div id="uncatched_exceptions_element">
</div>
<% unless(@downloading || @no_links) %>
 <span style="margin-right:10px">

 </span>
<% end %>

<script type="text/javascript">
//<![CDATA[





function onload_source_files_call() {
if (typeof(Layout) == 'undefined') { if ($('main-menubar')) { build_menubar('main-menubar') }}
}
<% if request.xhr? %>onload_source_files_call();<% else %>if (typeof(Event) != 'undefined' && Event.observe) {
Event.observe(window, 'load', onload_source_files_call)
} else if (typeof(jQuery) != 'undefined') {
$(window).load(onload_source_files_call)
}<% end %>


//]]>
</script>