function add_reports() {
  i = 1;
  while(Reports["url"+i]) {
    if (Reports["url"+i].match(/report_generate\.json/)) {
      swfobject.embedSWF("/open-flash-chart.swf", "div_chart_" + i, "600", "400", "9.0.0", "expressInstall.swf", {"get-data":"get_data", "id":i});
