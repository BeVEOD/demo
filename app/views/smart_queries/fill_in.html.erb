<label for="smart_query_<%=  @smart_query.id || 0  %>_table_sid" id="smart_queries_table_sid_<%= @smart_query.id || 0 %>"><%= s_(%q{Table: }) %><input id="smart_query_<%= @smart_query.id || 0 %>_table_sid" maxlength="255" name="smart_query[<%=  @smart_query.id || 0  %>][table_sid]" type="text" value="<%=h @smart_query.table_sid %>" /></label>
<label for="smart_query_<%=  @smart_query.id || 0  %>_joining_criteria" id="smart_queries_joining_criteria_<%= @smart_query.id || 0 %>"><%= s_(%q{Joining Criteria: }) %><select name="smart_query[<%=  @smart_query.id || 0  %>][joining_criteria]" id="smart_query_<%= @smart_query.id || 0 %>_joining_criteria" >
<%= options_for_select(@smart_query.joining_criteria__potential_values(@current_user).map{|p| [_(p), (SmartQuery::JOINING_CRITERIA.index(p) || -1 )+1] }, @smart_query.joining_criteria__index) %>
</select></label>

<p id="smart_queries_test_query_<%= @smart_query.id || 0 %>">Test Query</p>
</fieldset>

<input data-disable-with="<%= h(_(%q{Saving...})) %>" disable_with="Saving..." id="fill_in_submit_form_submit" name="commit" type="submit" value="<%= _(%q{Submit}) %>" />
